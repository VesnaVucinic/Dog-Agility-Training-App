<h1>Sign Up Here!</h1>

<%= form_for @user% do |f|!> #<!--in form_for I need to pass model-->
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Sign Up" %>
<% end %>
